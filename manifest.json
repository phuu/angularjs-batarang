{
  "name": "Flight Batarang",
  "version": "0.0.2",
  "description": "Extends the Developer Tools, adding tools for debugging and profiling Twitter Flight applications.",
  "background": {
    "page": "background.html"
  },
  "devtools_page": "devtoolsBackground.html",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [],
      "run_at": "document_start"
    }
  ],
  "minimum_chrome_version": "21.0.1180.57"
}
